					<key>COMMAND_STRING</key>
					<string>say "trash"

dest=~/Desktop/iOS\ Appsâ€¦
tmp_log=/tmp/trash-rescue.txt

[ -d "${dest}" ] || mkdir "${dest}"
echo "" &gt; ${tmp_log}

for item in "$@"; do
	echo "\$item=${item}" &gt;&gt; ${tmp_log}
	if [ $(expr "$item" : '.*\(\.ipa\)$') ]; then
		echo "saved!" &gt;&gt; ${tmp_log}
		mv "$item" "$dest" &amp;&amp; say "saved"
	else
		say "indeed"
	fi
done

